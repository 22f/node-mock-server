{
    "name": "node-mock-server-uuid",
    "version": "1.0.0",
    "private": false,
    "devDependencies": {
        "@namics/eslint-config": "4.1.0",
        "eslint-config": "0.1.0",
        "flow-bin": "0.61.0",
        "flow-typed": "2.2.3",
        "node-flow-config": "0.1.0",
        "npm-run-all": "4.1.1"
    },
    "main": "dist/index.js",
    "scripts": {
        "dev": "node-flow-config start",
        "build": "node-flow-config build",
        "test": "CI=true node-flow-config test --env=jsdom --coverage",
        "test:dev": "node-flow-config test --env=jsdom",
        "lint": "npm-run-all lint:*",
        "lint:js": "node_modules/.bin/eslint .",
        "flow": "node_modules/.bin/flow",
        "flow-restart": "node_modules/.bin/flow stop && node_modules/.bin/flow",
        "flow-install": "node_modules/.bin/flow-typed install -i dev"
    }
}
