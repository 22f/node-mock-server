    "engines": {
        "node": ">=6"
    },
    "main": "dist/server/index.js",
    "preferGlobal": false,
    "private": false,
    "license": "MIT",
    "dependencies": {
        "node-mock-server-api": "0.1.0",
        "node-mock-server-uuid": "1.0.0",
        "node-mock-server-utils": "0.1.0",
        "node-mock-server-log": "0.1.0",
        "opn": "5.1.0"
    },
    "devDependencies": {
        "@namics/eslint-config": "4.1.0",
        "eslint-config": "0.1.0",
        "flow-bin": "0.61.0",
        "flow-typed": "2.2.3",
        "node-flow-config": "0.1.0",
        "npm-run-all": "4.1.1",
        "express": "4.16.2",
        "axios": "0.17.1"
    },
    "scripts": {
        "dev": "node-flow-config start",
        "build": "node-flow-config build",
        "test": "CI=true node-flow-config test --env=jsdom",
