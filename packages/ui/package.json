{
    "name": "node-mock-server-ui",
    "version": "0.1.0",
    "private": false,
    "dependencies": {
        "custom-react-scripts": "0.2.0",
        "node-mock-server-uuid": "1.0.0",
        "react": "16.0.0",
        "react-dom": "16.0.0"
    },
    "devDependencies": {
        "@namics/eslint-config": "4.1.0",
        "eslint-config": "0.1.0",
        "flow-bin": "0.61.0",
        "flow-typed": "2.2.3",
        "node-flow-config": "0.1.0",
        "npm-run-all": "4.1.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "CI=true node-flow-config test --env=jsdom --coverage",
        "test:dev": "node-flow-config test --env=jsdom",
        "lint": "npm-run-all lint:*",
        "lint:js": "node_modules/.bin/eslint .",
        "flow": "node_modules/.bin/flow",
        "flow-version": "node_modules/.bin/flow version",
        "flow-install": "node_modules/.bin/flow-typed install -i dev",
        "flow-cache": "node_modules/.bin/flow-typed update-cache --debug"
    }
