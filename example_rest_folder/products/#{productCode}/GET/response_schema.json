{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Product",
  "type": "object",
  "properties": {
    "productCode": {
      "type": "string"
    },
    "price": {
      "type": "object",
      "properties": {
        "currency": "string",
        "price": {
          "type": "object",
          "properties": {
            "value": {
              "type": "number"
            }
          }
        }
      }
    },
    "email": {
      "type":"string"
    },
    "success": {
      "type":"boolean"
    },
    "highlight": {
      "type": "boolean"
    },
    "quantity": {
      "type": "integer",
      "format": "int32"
    },
    "currentPage": {
      "type":"string"
    },
    "card": {
      "type": "object",
      "properties": {
        "name": {
          "type":"string"
        },
        "username": {
          "type":"string"
        },
        "email": {
          "type":"string"
        },
        "phone": {
          "type":"string"
        },
        "website": {
          "type":"string"
        },
        "posts": {
          "type": "array",
          "items": {
            "properties": {
              "words": {
                "type":"string"
              },
              "sentence": {
                "type":"string"
              },
              "sentences": {
                "type":"string"
              },
              "paragraph": {
                "type":"string"
              }
            }
          }
        },
        "accountHistory": {
          "type": "array",
          "items": {
            "properties": {
              "name": {
                "type":"string"
              },
              "type": {
                "type":"string"
              },
              "account": {
                "type":"string"
              },
              "amount": {
                "type":"string"
              },
              "date": {
                "type":"string"
              },
              "business": {
                "type":"string"
              }
            }
          }
        },
        "address": {
          "type": "object",
          "properties": {
            "streetA": {
              "type":"string"
            },
            "streetB": {
              "type":"string"
            },
            "streetC": {
              "type":"string"
            },
            "streetD": {
              "type":"string"
            },
            "city": {
              "type":"string"
            },
            "state": {
              "type":"string"
            },
            "country": {
              "type":"string"
            },
            "zipcode": {
              "type":"string"
            },
            "geo": {
              "type": "object",
              "properties": {
                "lat": {
                  "type":"string"
                },
                "lng": {
                  "type":"string"
                }
              }
            },
            "company": {
              "type": "object",
              "properties": {
                "name": {
                  "type":"string"
                },
                "catchPhrase": {
                  "type":"string"
                },
                "bs": {
                  "type":"string"
                }
              }
            }
          }
        }
      }
    },
    "image": {
      "type": "object",
      "properties": {
        "url": {
          "type":"string"
        },
        "alt": {
          "type":"string"
        }
      }
    }
  }
}